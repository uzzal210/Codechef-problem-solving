#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int n,cnt=0,cnt1=0;
	    cin>>n;
	   
	        for(int i=1;i<=n;i=i+2)
	        {
	            if(n%i==0)
	            {
	               cnt++; 
	            }
	    }
	        for(int i=2;i<=n;i=i+2)
	            {
	                if(n%i==0)
	                {
	                    cnt1++; 
	                }
	            }
	       cout<<cnt<<" "<<cnt1<<endl;     
	       
	}
	return 0;

}
